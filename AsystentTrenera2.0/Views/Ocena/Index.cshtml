@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.OcenaId }) |
            @Html.ActionLink("Details", "Details", new { id=item.OcenaId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.OcenaId })
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.Mecz).ToString()
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.Pozycja).ToString()
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.Zawodnik).ToString()
        </td>
        <td>
			@(item.OcenaInnychs == null ? "None" : item.OcenaInnychs.Count.ToString())
        </td>
        <td>
			@String.Format("{0:F}", item.Ocen)
        </td>
        <td>
			@String.Format("{0:F}", item.Gole)
        </td>
        <td>
			@String.Format("{0:F}", item.Strzal)
